#! /usr/bin/env python
import sys
from curses.ascii import controlnames 

if len(sys.argv)==1: 
    print "toASCII STDIN:  Print hex-numbers from STDIN as characters,\n\t\tconvert control signals to mnemonics"
    sys.exit(0)
repStr = "'"
for ch in sys.argv[1:]: 
    nr = int(ch,16)
    if nr < 32:
    	repStr += "<"+controlnames[nr]+">"
    elif nr < 127:
    	repStr += chr(nr)
    elif nr == 127:
    	repStr += "<DEL>"
    else:
    	repStr += chr(nr)

print repStr+"'"
